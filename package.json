  "scripts": {
    "compile": "tsc --noEmit -p . --pretty",
    "format": "eslint . --fix",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "patch": "patch-package",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:maestro": "maestro test .maestro/FavoritePodcast.yaml",
